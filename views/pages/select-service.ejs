                  var _getStylesheetDirectoryUri = '../wp-content/themes/cwiz-wp-theme/index.html';  </script>
    <script id="cwiz-bundle" type="text/javascript" src="../wp-content/themes/cwiz-wp-theme/js/libs.bundle.js"></script>
