<style>
  a,
  body,
  div,
  h3,
  h5,
  html,
  img,
  section {
    vertical-align: baseline;
  }
  
  body,
  div,
  html,
  img,
  section {
    margin: 0;
    padding: 0;
  }
  
  html body .logo_desktop_v {
    display: inline-block;
    background: url("https://thebfe.com/assets/svg/common/logo_desktop.svg")
      center center/cover no-repeat;
    transition: 0.6s;
  }
  
  html body .logo_desktop_v:hover {
    transform: scale(0.98);
    opacity: 0.5;
  }
  
  .components.header__logo {
    opacity: 0;
    position: relative;
    text-align: center;
    padding-top: 60px;
    padding-bottom: 40px;
  }
  
  @media (max-width: 799px) {
    .components.header__logo {
      padding-bottom: 60px;
    }
  
    .components.header__logo .logo_desktop_v {
      display: none;
    }
  }
  
  body section.contents .contents__wrapper,
  body.page-template-page-Services section.contents .contents__wrapper {
    max-width: 1100px !important;
    padding-right: 0 !important;
    padding-left: 0 !important;
  }
  
  a,
  div,
  img,
  section {
    border: 0;
    font: inherit;
    font-size: 100%;
  }
  
  a img {
    border: none;
  }
  
  html body a {
    text-decoration: none;
    list-style: none;
  }
  
  
  
  html body a:active,
  html body a:focus,
  html body img:active,
  html body img:focus {
    outline: 0;
    border: 0;
    -moz-outline-style: none;
  }
  
  html body img {
    vertical-align: top;
  }
  
  
  
  html body .logo_mobile_v {
    display: inline-block;
    background: url("https://thebfe.com/assets/svg/common/logo_mobile.svg") center
      center/contain no-repeat;
  }
  
  .components.header__mobile {
    position:fixed;
    top: 0;
    left: 0;
    text-align: center;
    width: 100%;
    height: 66px;
  }
  
  .components.header__mobile .logo_mobile_v {
    margin-top: 14px;
    margin-left: 30px;
  }
  
  h3 {
    text-transform: uppercase;
    font: inherit;
    color: var(--primary);
  }
  
  /*! CSS Used from: http://127.0.0.1:3001/assets/styles/index.css */
  body,
  h3,
  h5,
  html {
    border: 0;
  }
  
  
  html body #sitewrapper .contents__wrapper {
    position: relative;
    max-width: 1270px;
    margin: 0 auto;
    padding: 0 20px;
  }
  
  section {
    display: block;
  }
  
  body,
  html {
    font: inherit;
    height: 100%;
    font-size: 100%;
  }
  
  html body #sitewrapper section {
    position: relative;
    min-height: 500px;
    padding-bottom: 300px;
  }
  
  
  
  @media (min-width: 800px) {
    #mobileIcon,
    .components.header__mobile {
      display: none;
    }
  
    html body #sitewrapper {
      margin-left: 70px;
    }
  }
  
  html body {
    font-size: 1rem;
    line-height: 1.625rem;
    letter-spacing: 0.05em;
  }
  
  html {
    line-height: 1;
    -webkit-text-size-adjust: 100%;
  }
  
  .contents__block.cblock1 {
    text-align: center;
    max-width: 700px;
    margin: 0 auto;
    padding-right: 20px;
    padding-left: 20px;
    padding-bottom: 40px;
  }
  
  @media (min-width: 768px) {
    .contents__block.cblock1 {
      padding-bottom: 80px;
    }
  }
  
  .contents__block {
    position: relative;
  }
  
  h3 {
    margin: 0;
    padding: 0 0 30px;
    font: inherit;
    font-size: 1.125rem;
    line-height: 1.5rem;
    letter-spacing: 0.2em;
  }
  
  .headercopy {
    font-family: LibreBaskerville-Regular;
    font-size: 1.125rem;
    line-height: 2.375rem;
  }
  
  h5 {
    margin: 0 0 20px;
    padding: 0;
    font: inherit;
    font-size: 100%;
  }
  
